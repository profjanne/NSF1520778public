<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:keepScreenOn="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

 <TextView
     android:id="@+id/textView1"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_gravity="left"
     android:layout_marginBottom="5dp"
     android:text="62. Which group, or groups, best describe your race or ethnic origin? (Tell me all that apply)"
     android:textAppearance="?android:attr/textAppearanceMedium" />

    	<CheckBox
    	    android:id="@+id/chk1"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_marginBottom="5dp"
    	    android:button="@drawable/checkboxselector"
    	    android:text="Black/African American"
    	    android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="White"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Latino/Hispanic/Latin American"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Asian Indian/South Asian"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Chinese"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Filipino"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Japanese"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:button="@drawable/checkboxselector"
            android:text="Korean"
            android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk9"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Vietnamese"
           android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk10"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Other Asian"
           android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk11"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Native American/American Indian"
           android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk12"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Inuit/Eskimo/Aleut"
           android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk13"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Native Hawaiian"
           android:textAppearance="?android:attr/textAppearanceMedium" />

       <CheckBox
           android:id="@+id/chk14"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Samoan"
           android:textAppearance="?android:attr/textAppearanceMedium" />
       
       <CheckBox
           android:id="@+id/chk15"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginBottom="5dp"
           android:button="@drawable/checkboxselector"
           android:text="Other Pacific Islander"
           android:textAppearance="?android:attr/textAppearanceMedium" />

                 <RelativeLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content" >

                    <CheckBox
                        android:id="@+id/chk16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:button="@drawable/checkboxselector"
                        android:text="Some Other Race, SPECIFY"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                   <LinearLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@android:color/transparent"
    android:focusable="true"
    android:focusableInTouchMode="true">
</LinearLayout>
                    <EditText
                        android:id="@+id/other1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@+id/chk16" >
                    </EditText>

                </RelativeLayout>
             <RelativeLayout
        		android:id="@+id/RelativeLayout1"
        		android:layout_width="match_parent"
        		android:layout_height="wrap_content" >
             		<Button
            			android:id="@+id/Button_back"
            			android:layout_width="@dimen/button_width"
            			android:layout_height="@dimen/button_height"
           	 			android:layout_alignParentLeft="true"
        	    		android:layout_alignParentTop="true"
            			android:layout_gravity="left"
            			android:layout_weight="0"
            			android:onClick="back_button"
            			android:text="Back" />

        		<Button
            			android:id="@+id/Button_next"
            			android:layout_width="@dimen/button_width"
            			android:layout_height="@dimen/button_height"
            			android:layout_alignParentRight="true"
            			android:layout_alignParentTop="true"
            			android:layout_weight="0"
            			android:onClick="next_button"
            			android:text="Next" />
        	</RelativeLayout>

</LinearLayout>
