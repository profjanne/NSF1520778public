<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:keepScreenOn="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="C. Now, we would like to ask you about your feelings and thoughts."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginBottom="5dp"
            android:text="44. How are you feeling right now?  (Check all that apply)"
            android:textAppearance="?android:attr/textAppearanceMedium" />
<RelativeLayout
        		android:id="@+id/RelativeLayout2"
        		android:layout_width="match_parent"
        		android:layout_height="wrap_content" >
    	<CheckBox
    	    android:id="@+id/chk1"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_marginLeft="40dp"
    	    android:button="@drawable/checkboxselector"
    	    android:text="Anxious"
    	    android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk1"
            android:layout_below="@+id/chk1"
            android:button="@drawable/checkboxselector"
            android:text="Afraid"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk2"
            android:layout_below="@+id/chk2"
            android:button="@drawable/checkboxselector"
            android:text="Calm"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk3"
            android:layout_below="@+id/chk3"
            android:button="@drawable/checkboxselector"
            android:text="Fearful"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk4"
            android:layout_below="@+id/chk4"
            android:button="@drawable/checkboxselector"
            android:text="Relaxed"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk5"
            android:layout_below="@+id/chk5"
            android:button="@drawable/checkboxselector"
            android:text="Restful"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk6"
            android:layout_below="@+id/chk6"
            android:button="@drawable/checkboxselector"
            android:text="Edgy"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk7"
            android:layout_below="@+id/chk7"
            android:button="@drawable/checkboxselector"
            android:text="At ease"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk8"
            android:layout_below="@+id/chk8"
            android:button="@drawable/checkboxselector"
            android:text="Social"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk9"
            android:layout_below="@+id/chk9"
            android:button="@drawable/checkboxselector"
            android:text="Open"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk11"
            android:layout_below="@+id/chk11"
            android:button="@drawable/checkboxselector"
            android:text="Lively"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk12"
            android:layout_below="@+id/chk12"
            android:button="@drawable/checkboxselector"
            android:text="Friendly"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk13"
            android:layout_below="@+id/chk13"
            android:button="@drawable/checkboxselector"
            android:text="Nervous"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk14"
            android:layout_below="@+id/chk14"
            android:button="@drawable/checkboxselector"
            android:text="Uncertain"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk15"
            android:layout_below="@+id/chk15"
            android:button="@drawable/checkboxselector"
            android:text="Outgoing"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk16"
            android:layout_below="@+id/chk16"
            android:button="@drawable/checkboxselector"
            android:text="Guarded"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <CheckBox
            android:id="@+id/chk18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk17"
            android:layout_below="@+id/chk17"
            android:button="@drawable/checkboxselector"
            android:text="Tense"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk18"
            android:layout_below="@+id/chk18"
            android:button="@drawable/checkboxselector"
            android:text="Unafraid"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/chk19"
            android:layout_below="@+id/chk19"
            android:button="@drawable/checkboxselector"
            android:text="Worked up"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <CheckBox
            android:id="@+id/chk11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="100dp"
            android:button="@drawable/checkboxselector"
            android:text="Uneasy"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        </RelativeLayout>
                 <RelativeLayout
        		android:id="@+id/RelativeLayout3"
        		android:layout_marginTop="20dp"
        		android:layout_width="match_parent"
        		android:layout_height="wrap_content" >
             		<Button
            			android:id="@+id/Button_back"
            			android:layout_width="@dimen/button_width"
            			android:layout_height="@dimen/button_height"
           	 			android:layout_alignParentLeft="true"
        	    		android:layout_alignParentTop="true"
            			android:layout_gravity="left"
            			android:layout_weight="0"
            			android:onClick="back_button"
            			android:text="Back" />

        		<Button
            			android:id="@+id/Button_next"
            			android:layout_width="@dimen/button_width"
            			android:layout_height="@dimen/button_height"
            			android:layout_alignParentRight="true"
            			android:layout_alignParentTop="true"
            			android:layout_weight="0"
            			android:onClick="next_button"
            			android:text="Next" />
        	</RelativeLayout>

</LinearLayout>